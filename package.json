{
  "name": "mapi",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "run-p dev server ",
    "start-test": "run-p server-test dev",
    "server": "nodemon --inspect=9229 -e js,json,yaml -w config -w server server/server.js",
    "server-test": "cross-env NODE_ENV=test npm run server",
    "dev": "nodemon -w config -w scripts -w mock/mock-map.js -w client/theme scripts/server.js",
    "lint": "eslint . --fix --format codeframe",
    "lint-staged": "lint-staged",
    "test": "jest --forceExit",
    "check": "npm-check --update --skip-unused",
    "precommit": "lint-staged",
    "build": "cross-env NODE_ENV=production node scripts/compile.js",
    "deploy": "cross-env NODE_ENV=production node scripts/deploy.js",
    "dll": "webpack --config config/webpack.config.dll.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:anthonyli/mAPI.git"
  },
  "license": "MIT",
  "lint-staged": {
    "**/*.js": [
      "eslint --fix --format codeframe",
      "git add"
    ]
  },
